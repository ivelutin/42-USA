FLAGS = -Wall -Wextra -Werror
NAME = EVALEXPR

all: $(NAME)

$(NAME):
		gcc $(FLAGS) main.c ft_jim.c ft_calc.c  -o evalexpr


clean:
	rm -f evalexpr
